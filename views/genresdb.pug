extends htmlTemplate.pug
block breadcrumb
    li.breadcrumb-item.active GenreDB
block content
    .container
        table.table.table-hover
            thead
                tr
                    th No.
                    th _id
                    th genreName
                    th imageLink
            tbody
                each genre, index in genres
                    tr
                        td= index+1
                        td= genre.genreID
                        td= genre.name
                        td= genre.image